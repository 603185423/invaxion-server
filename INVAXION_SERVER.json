{
 "apps": [
  {
   "name": "invaxion",
   "script": "./src/index.js",
   "log_date_format": "YYYY-MM-DD HH:mm:ss",
   "log_file": "./logs/invaxion.log",
   "error_file": "./logs/invaxion-err.log",
   "out_file": "./logs/invaxion-out.log",
   "pid_file": "./pids/invaxion.pid",
   "instances": 1,
   "min_uptime": "200s",
   "watch": false,
   "merge_logs": true,
   "exec_interpreter": "node",
   "exec_mode": "fork",
   "autorestart": true,
   "vizion": false
  }
 ]
}
